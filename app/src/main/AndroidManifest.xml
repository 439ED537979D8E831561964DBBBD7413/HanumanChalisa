<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chalisa.gamedesigna.hanumanchalisaa.hanumanchalisa">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ii"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ii"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="hanumanchalisa.gstech.com.hanumanchalisa.MainActivity">
            <intent-filter>


                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="hanumanchalisa.gstech.com.hanumanchalisa.Lyrics_activity" />


        <activity android:name="hanumanchalisa.gstech.com.hanumanchalisa.Splash_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>